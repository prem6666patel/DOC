{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/file/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/user/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/consultation/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/health",
      "dest": "server.js",
      "methods": ["GET"]
    }
  ]
}